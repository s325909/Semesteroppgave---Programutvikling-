<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="CENTER" hgap="10" prefHeight="720.0" prefWidth="1280.0" style="-fx-background-color: BLACK" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="menuOptions.CreditsController">

    <stylesheets><URL value="@StylesMenu.css" /></stylesheets>
   <children>
   
       <VBox fx:id="settingsBackground" alignment="CENTER" prefHeight="720" prefWidth="1280">
      <children>
      
              <Label text="Credits" textAlignment="CENTER" textFill="RED">
                  <font>
                      <Font name="Copperplate Gothic Light" size="64.0" />
                  </font>
              </Label>
            <HBox alignment="CENTER" prefHeight="487.0" prefWidth="1280.0">
               <children>
                  <TextArea fx:id="creditsText" prefHeight="487.0" prefWidth="658.0">
                     <font>
                        <Font size="14.0" />
                     </font></TextArea>
               </children>
            </HBox>
            <VBox alignment="BOTTOM_CENTER">
               <children>
                  <Button fx:id="backToMenu" mnemonicParsing="false" onAction="#returnToMenu" prefHeight="75.0" prefWidth="250.0" text="Back to Menu">
                     <font>
                        <Font name="Copperplate Gothic Light" size="24.0" />
                     </font>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets top="25.0" />
               </VBox.margin>
            </VBox>
      </children>
   
       </VBox>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
